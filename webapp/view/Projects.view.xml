<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="programmeMotse.controller.Projects">
	<App>
		<pages>
			<Page>
				<content>
					<ObjectHeader title="Add Projects" introActive="true" titleActive="true" icon="sap-icon://add-activity"/>
					<Table id="projectTable" noDataText="Click the &quot;+&quot; to add a project">
						<columns>
							<Column>
								<header>
									<Label text="Project Name"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="Start Date"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="End Date"/>
								</header>
							</Column>
						</columns>
						<headerToolbar>
							<Toolbar width="100%">
								<content>
									<ToolbarSpacer/>
									<Button width="auto" icon="sap-icon://add" type="Emphasized"/>
								</content>
							</Toolbar>
						</headerToolbar>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>