<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="programmeMotse.controller.Projects">
	<App>
		<pages>
			<Page>
				<content>
				    <ObjectHeader title="Add Projects" introActive="true" titleActive="true" icon="sap-icon://add-activity"/>
				    <Table noDataText="Click the &quot;+&quot; to add a project">
				        <columns>
				            <Column>
				                <header>
				                    <Label text="Project Name" id="__label0"/>
				                </header>
				            </Column>
				            <Column id="__column1">
				                <header>
				                    <Label text="Start Date" id="__label1"/>
				                </header>
				            </Column>
				            <Column id="__column2">
				                <header>
				                    <Label text="End Date" id="__label2"/>
				                </header>
				            </Column></columns>
				        <headerToolbar>
				            <Toolbar width="100%" id="__toolbar1">
				                <content>
				                    <ToolbarSpacer id="__spacer0"/>
				                    <Button width="auto" icon="sap-icon://add" type="Emphasized"/>
				                </content>
				            </Toolbar>
				        </headerToolbar></Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>